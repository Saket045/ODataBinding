<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
  controllerName="library.controller.CreateStudent">
  <Page >
	<VBox class="sapUiSmallMargin">
		<f:Form id="FormChangeColumn_oneGroup2345"
			editable="true">
			<f:title>
				<core:Title text="Student" />
			</f:title>
			<f:layout>
				<f:ColumnLayout
					columnsM="2"
					columnsL="3"
					columnsXL="4"
				/>
			</f:layout>
			<f:formContainers>
				<f:FormContainer>
					<f:formElements>
						<f:FormElement label="StudentId">
							<f:fields>
								<Input value="{form>/StudentId}" id="StudentId" change="onInputChange" name="StudentId" />
							</f:fields>
						</f:FormElement>
			<f:FormElement label="Name">
							<f:fields>
								<Input value="{form>/StudentName}" id="Name" change="onInputChange" name="StudentName"/>
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Email">
							<f:fields>
								<Input value="{form>/Email}" change="onInputChange" name="Email" />
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Phone">
							<f:fields>
								<Input value="{form>/Phone}" change="onInputChange" name="Phone" />
							</f:fields>
						</f:FormElement>
						<f:FormElement label="Address">
							<f:fields>
								<Input value="{form>/Address}" change="onInputChange" name="Address" />
							</f:fields>
						</f:FormElement>
					
				
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
  <footer >
    <Toolbar>
      <ToolbarSpacer />
      <Button text="Cancel" type="Reject" press="onCancelPress" />
      <Button text="Save" type="Emphasized" press="onSavePress" class="sapUiLargeMarginEnd" />
    </Toolbar>
  </footer>
  </Page>
</core:FragmentDefinition>